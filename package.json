{
  "name": "use-validation-form",
  "version": "1.0.1",
  "description": "React hook for form input handling and validation",
  "author": "martinhackl",
  "license": "MIT",
  "repository": "martinhackl/use-validation-form",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom --coverage",
    "test:watch": "react-scripts test --env=jsdom",
    "lint": "eslint src --ext .ts,.tsx",
    "build": "rollup -c",
    "build:watch": "nodemon -x npm run build --watch src -e ts,tsx",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "files": [ "dist" ],
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@testing-library/react": "^9.1.4",
    "@testing-library/react-hooks": "^2.0.1",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.22",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.4.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "gh-pages": "^2.0.1",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.9.0",
    "react-scripts": "^3.0.1",
    "react-select-event": "^2.1.1",
    "react-test-renderer": "^16.9.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.19.2",
    "rollup-plugin-url": "^2.1.0",
    "typescript": "^3.2.4"
  }
